                                "env": [
                                    {
                                        "name": "DB_DATABASE",
                                        "value": "${APP_DB_DATABASE}"
                                    },
                                    {
                                        "name": "DB_HOST",
                                        "value": "${APP_NAME}-db"
                                    },
                                    {
                                        "name": "DB_PASSWORD",
                                        "value": "${APP_DB_PASSWORD}"
                                    },
                                    {
                                        "name": "DB_USERNAME",
                                        "value": "${APP_DB_USER}"
                                    },
                                    {
                                        "name": "RABBITMQ_HOST",
                                        "value": "${APP_NAME}-mq"
                                    },
                                    {
                                        "name": "RABBITMQ_LOGIN",
                                        "value": "${APP_MQ_USER}"
                                    },
                                    {
                                        "name": "RABBITMQ_PASSWORD",
                                        "value": "${APP_MQ_PASSWORD}"
                                    },
                                    {
                                        "name": "WAIT_FOR_DB_MIGRATIONS",
