                    {
                        "name": "amqps",
                        "port": 5671,
                        "protocol": "TCP",
                        "targetPort": "amqps"
                    },
                    {
                        "name": "http",
                        "port": 80,
                        "protocol": "TCP",
                        "targetPort": "http"
                    },
                    {
                        "name": "https",
                        "port": 443,
                        "protocol": "TCP",
                        "targetPort": "https"
                    }
                ],
                "selector": {
                    "name": "${APP_NAME}-mq"
