                },
                "strategy": {
                    "type": "Rolling"
                },
                "template": {
                    "metadata": {
                        "labels": {
                            "app": "${APP_NAME}",
                            "component": "worker",
                            "name": "${APP_NAME}-wk",
                            "template": "laravel-application"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "args": [
                                    "application-worker"
                                ],
                                "env": [
                                    {
                                        "name": "DB_DATABASE",
                                        "value": "${APP_DB_DATABASE}"
                                    },
                                    {
                                        "name": "DB_HOST",
                                        "value": "${APP_NAME}-db"
                                    },
                                    {
                                        "name": "DB_PASSWORD",
                                        "value": "${APP_DB_PASSWORD}"
                                    },
                                    {
                                        "name": "DB_USERNAME",
                                        "value": "${APP_DB_USER}"
                                    },
                                    {
                                        "name": "RABBITMQ_HOST",
                                        "value": "${APP_NAME}-mq"
                                    },
                                    {
                                        "name": "RABBITMQ_LOGIN",
                                        "value": "${APP_MQ_USER}"
                                    },
                                    {
                                        "name": "RABBITMQ_PASSWORD",
                                        "value": "${APP_MQ_PASSWORD}"
                                    },
                                    {
                                        "name": "WAIT_FOR_DB_MIGRATIONS",
                                        "value": "true"
                                    }
                                ],
                                "image": "astrolox/ubuntu-16-laravel-application:latest",
                                "imagePullPolicy": "Always",
                                "name": "${APP_NAME}-wk",
                                "ports": [],
                                "volumeMounts": [
                                    {
                                        "mountPath": "/tmp",
                                        "name": "${APP_NAME}-wk-tmp"
                                    }
                                ]
                            }
                        ],
                        "restartPolicy": "Always",
                        "securityContext": {
                            "runAsUser": 1099
                        },
                        "volumes": [
                            {
                                "emptyDir": null,
                                "name": "${APP_NAME}-wk-tmp"
                            }
                        ]
                    }
                },
                "triggers": [
                    {
                        "type": "ConfigChange"
