        {
            "apiVersion": "v1",
            "kind": "DeploymentConfig",
            "metadata": {
                "labels": {
                    "app": "${APP_NAME}",
                    "component": "database",
                    "name": "${APP_NAME}-db",
                    "template": "laravel-application"
                },
                "name": "${APP_NAME}-db"
            },
            "spec": {
                "replicas": 1,
                "selector": {
                    "name": "${APP_NAME}-db"
                },
                "strategy": {
                    "type": "Recreate"
                },
                "template": {
                    "metadata": {
                        "labels": {
                            "app": "${APP_NAME}",
                            "component": "database",
                            "name": "${APP_NAME}-db",
                            "template": "laravel-application"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "env": [
                                    {
                                        "name": "MYSQL_DATABASE",
                                        "value": "${APP_DB_DATABASE}"
                                    },
                                    {
                                        "name": "MYSQL_PASSWORD",
                                        "value": "${APP_DB_PASSWORD}"
                                    },
                                    {
                                        "name": "MYSQL_ROOT_PASSWORD",
                                        "value": "${APP_DB_ROOT_PASSWORD}"
                                    },
                                    {
                                        "name": "MYSQL_USER",
                                        "value": "${APP_DB_USER}"
                                    },
                                    {
                                        "name": "PMA_CONTROL_PASSWORD",
                                        "value": "${APP_DB_ROOT_PASSWORD}"
                                    }
                                ],
                                "image": "1and1internet/ubuntu-16-nginx-php-phpmyadmin-mysql-5:latest",
                                "imagePullPolicy": "Always",
                                "livenessProbe": {
                                    "exec": {
                                        "command": [
                                            "/bin/sh",
                                            "-i",
                                            "-c",
                                            "MYSQL_PWD=\"$MYSQL_PASSWORD\"  mysql -h 127.0.0.1 -u $MYSQL_USER -D $MYSQL_DATABASE -e 'SELECT 1'"
                                        ]
                                    },
                                    "failureThreshold": 3,
                                    "initialDelaySeconds": 10,
                                    "periodSeconds": 10,
                                    "successThreshold": 1,
                                    "timeoutSeconds": 2
                                },
                                "name": "${APP_NAME}-db",
                                "ports": [
                                    {
                                        "containerPort": 3306,
                                        "name": "mysql",
                                        "protocol": "TCP"
                                    },
                                    {
                                        "containerPort": 8080,
                                        "name": "http",
                                        "protocol": "TCP"
                                    },
                                    {
                                        "containerPort": 8443,
                                        "name": "https",
                                        "protocol": "TCP"
                                    }
                                ],
                                "readinessProbe": {
                                    "exec": {
                                        "command": [
                                            "/bin/sh",
                                            "-i",
                                            "-c",
                                            "MYSQL_PWD=\"$MYSQL_PASSWORD\"  mysql -h 127.0.0.1 -u $MYSQL_USER -D $MYSQL_DATABASE -e 'SELECT 1'"
                                        ]
                                    },
                                    "failureThreshold": 3,
                                    "initialDelaySeconds": 10,
                                    "periodSeconds": 10,
                                    "successThreshold": 1,
                                    "timeoutSeconds": 2
                                },
                                "volumeMounts": [
                                    {
                                        "mountPath": "/var/lib/mysql",
                                        "name": "${APP_NAME}-db-data"
                                    },
                                    {
                                        "mountPath": "/tmp",
                                        "name": "${APP_NAME}-db-tmp"
                                    }
                                ]
                            }
                        ],
                        "restartPolicy": "Always",
                        "securityContext": {
                            "runAsUser": 1099
                        },
                        "volumes": [
                            {
                                "hostPath": {
                                    "path": "${STORAGE_ROOT}/${APP_NAME}-db-data"
